{
  "name": "hexel-test",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index",
  "scripts": {
    "order-book": "node ./dist/order_book",
    "start": "node ./dist/index.js",
    "start-dev": "ts-node ./src/index.ts",
    "build": "npm run build:clean && npm run build:lib && npm run lint",
    "build:clean": "rimraf dist",
    "build:lib": "tsc",
    "test": "nyc --nycrc-path .nyrc_unit npm run test:unit",
    "test:unit": "mocha --compilers ts-node/register --require source-map-support/register --full-trace \"test/**/*.unit.ts\"  --timeout 30000",
    "test:e2e": "nyc --nycrc-path .nyrc_e2e mocha --compilers ts-node/register --require source-map-support/register --full-trace \"test/**/*.e2e.ts\" --timeout 30000",
    "lint": "tslint \"src/**/*.ts\"",
    "lint-fix": "tslint --fix \"src/**/*.ts\"",
    "db:migrate": "sequelize db:migrate",
    "db:seed:all": "sequelize db:seed:all",
    "db:newmigration": "sequelize migration:create --name $1",
    "test:all": "npm run lint && npm run build && npm run test && npm run test:e2e"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.0.10",
    "@types/chai-as-promised": "^7.1.0",
    "@types/decamelize": "^1.2.0",
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.91",
    "@types/mocha": "^2.2.45",
    "@types/node": "^8.0.17",
    "@types/proxyquire": "^1.3.28",
    "@types/request-promise-native": "^1.0.14",
    "@types/sequelize": "^4.0.80",
    "@types/sinon": "^4.3.1",
    "@types/sqlite3": "^3.1.3",
    "@types/string": "^0.0.29",
    "@types/supertest": "^2.0.4",
    "@types/umzug": "^2.2.0",
    "@types/uuid": "^3.4.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "mocha": "^5.2.0",
    "nyc": "^11.6.0",
    "proxyquire": "^2.0.1",
    "request": "^2.85.0",
    "rimraf": "*",
    "sinon": "^4.5.0",
    "supertest": "^3.0.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.2",
    "typescript-eslint-parser": "^11.0.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ],
    "globals": [
      "sandbox",
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "decamelize": "^2.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.15.2",
    "lodash": "^4.17.10",
    "moment": "^2.18.1",
    "mysql2": "^1.6.1",
    "sequelize": "^4.15.0",
    "sequelize-cli": "^4.0.0",
    "umzug": "^2.1.0",
    "uuid": "^3.3.2"
  }
}
